{
    "go test": {
        "prefix": [
            "test"
        ],
        "body": [
            "func Test$1 (t *testing.T) { \n",
            "//test stuff",
            "}"
        ],
        "description": "Creates a golang test"
    },
    "go func": {
        "prefix": [
            "gofunc"
        ],
        "body": [
            "go func() { \n",
            "}()"
        ],
        "description": "Creates an anonymous go routine"
    },
    "for select": {
        "prefix": [
            "forsel"
        ],
        "body": [
            "for {",
            "\tselect {",
            "\tcase $1:",
            "\t\t$0",
            "\t}",
            "}"
        ],
        "description": "Creates a golang test"
    }
}
